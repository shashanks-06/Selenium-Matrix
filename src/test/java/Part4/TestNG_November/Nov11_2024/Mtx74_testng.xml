<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<!--For Parallel Cross Browser Testing <suite parallel="tests">-->

<suite name="All Test Suite" parallel="tests">

<!--          Name in <test> is IMPORTANT, They should be different -->
    <test verbose="2" preserve-order="true"
          name="For Chrome Browser">

        <parameter name="browserName" value="Chrome"/>
        <classes>
            <class name="Part4.TestNG_November.Nov11_2024.Mtx74_CrossBrowserTesting"/>
        </classes>
    </test>

<!--          Name in <test> is IMPORTANT, They should be different -->
    <test verbose="2" preserve-order="true"
          name="For Edge Browser">
        <parameter name="browserName" value="Edge"/>
        <classes>
            <class name="Part4.TestNG_November.Nov11_2024.Mtx74_CrossBrowserTesting"/>
        </classes>
    </test>
</suite>